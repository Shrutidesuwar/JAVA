import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner sc=new Scanner (System.in);
        int Size=sc.nextInt();
        int key=sc.nextInt();

        int [] arr=new int[Size];

        for(int i=0;i<Size;i++){
            arr[i]=sc.nextInt();
        }

        for(int i=0;i<Size;i++) {
            if (arr[i] == key) {
                System.out.println(i);
            }
        }

    }
}
